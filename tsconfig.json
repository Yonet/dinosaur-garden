{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5"
    ],
    "jsx": "react",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "preserveConstEnums": true
  },
  "types": [
    "babylonjs",
    "babylonjs-gui",
    "babylonjs-loaders",
    "angularFTW"
  ],
}